@model IEnumerable<School.student>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.studname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sub)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.studname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sub)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.studid }) |
            @Html.ActionLink("Details", "Details", new { id=item.studid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.studid })
        </td>
    </tr>
}

</table>
